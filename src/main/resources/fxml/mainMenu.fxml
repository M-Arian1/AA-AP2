<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginMenu">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="350.0" layoutY="179.0" prefHeight="200.0" prefWidth="139.0" spacing="25.0">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#checkNewGame" text="New Game" />
            <Button mnemonicParsing="false" onMouseClicked="#checkLoadGame" text="Load Game" />
            <Button mnemonicParsing="false" onMouseClicked="#checkProfile" text="Profile" />
            <Button mnemonicParsing="false" onMouseClicked="#checkHighScores" text="HighScores" />
            <Button mnemonicParsing="false" onMouseClicked="#checkSettings" text="Settings" />
            <Button mnemonicParsing="false" onMouseClicked="#checkExit" text="Exit" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
